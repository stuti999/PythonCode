data=[]

name=input("enter a name ").split("=")
data.append(name)
address=input("enter a address").split("=")
data.append(address)
coures=input("enter a address").split("=")
data.append(coures)
ph_no=int(input("enter a number").split("=")
data.append(ph_no).split("=")
print(data)
info=dict(data)
   
